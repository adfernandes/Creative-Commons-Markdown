{
	"name": "cc-md",
	"version": "0.4.1",
	"description": "Markdown-formatted Creative Commons licenses",
	"author": "idleberg",
	"license": "CC0-1.0",
	"files": [
		"1.0/",
		"2.0/",
		"2.5/",
		"3.0/",
		"4.0/",
		"legacy/",
		"LICENSE",
		"readme.markdown"
